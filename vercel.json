{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
    "src": "**/*.html", 
    "use": "@vercel/static"
  }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/submit-form",
      "dest": "server.js"
    },
    {
      "src": "/registrationform.html",
      "dest": "/registrationform.html"
    },
    {
      "src": "/admin",
      "dest": "server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
